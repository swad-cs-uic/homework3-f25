{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "emulators": "npx firebase emulators:start --only firestore,auth --import=.emulator-data --export-on-exit",
    "dev": "concurrently -k -n EMUS,WEB \"npm:emulators\" \"npm:dev:vite\"",
    "emu:start": "npx firebase emulators:start --only firestore,auth --project demo-test --import=.emulator-data --export-on-exit",
    "emu:wait": "wait-on tcp:127.0.0.1:8080 tcp:127.0.0.1:9099",
    "test:integration": "concurrently -k -n EMU,TEST \"npm:emu:start\" \"npm:test\"",
    "test": "vitest"
  },
  "dependencies": {
    "firebase": "^12.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^9.9.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "firebase-tools": "^14.19.1",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "react-window": "^2.1.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  }
}